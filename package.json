{
  "name": "redis-examples",
  "version": "1.0.0",
  "description": "Repositorio para praticar redis",
  "main": "src/index.ts",
  "repository": "https://github.com/iago-f-s-e/redis-examples.git",
  "author": "iago fagundes <iago.fagundes@outlook.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "yarn build && yarn start:prod",
    "start:prod": "node dist/index.js",
    "start:dev": "env NODE_ENV=test ts-node-dev -r tsconfig-paths/register --respawn --inspect --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "test": "env NODE_ENV=test yarn test:unit && yarn test:functional",
    "test:functional": "env NODE_ENV=test jest --projects ./test --runInBand",
    "test:unit": "env NODE_ENV=test jest --detectOpenHandles",
    "typeorm": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js",
    "typeorm:generate": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js migration:generate -n",
    "typeorm:run": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js migration:run",
    "typeorm:revert": "ts-node-dev -r tsconfig-paths/register node_modules/typeorm/cli.js migration:revert"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "express": "^4.17.1",
    "typeorm": "^0.2.39"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.16.0",
    "@babel/plugin-transform-flow-strip-types": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/node": "15.12.2",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  }
}
